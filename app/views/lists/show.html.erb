<h1><%= @list.title %></h1>

<h4>Tasks</h4>
<ul>
  <% @list.tasks.default.each do |task| %>
    <li><h5><%= task.title %></h5>
      <p>
      Start Date: <%= task.start_date %>
      <br>
      Due Date: <%= task.due_date %>
      </p>
      <%= link_to "Edit", edit_list_task_path(@list, task.id) %>
    </li>
  <% end %>
</ul>
<%= link_to "New Task", new_list_task_path(@list) %>
<%= link_to "Back to All Lists", root_path %>