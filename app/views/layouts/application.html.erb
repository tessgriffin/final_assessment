<!DOCTYPE html>
<html>
<head>
  <title>FinalAssessment</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <% flash.each do |type, message| %>
    <%= content_tag :div, message %>
  <% end %>
  <div class="container">
    <%= yield %>
  </div>
</body>
</html>
